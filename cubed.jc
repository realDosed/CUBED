        PrtlResetVelocity = "1";
        TeleportPos = PrtlLinkedCenterPos.shift_location_in_direction("1.8");
    }
    if(s`%player%_AltVelocitySetting`.equals([0])){
        MaxVelocity = PlayerVelocity;
        if(PrtlResetVelocity.equals(["1"])){
