                else{
                    g`%var_local(ActivatorID)_IsActive` = 0;
                    g`%var_local(ActivatorID)_CurrentPointList` = 0;
                }
            }
            else{
                g`%var_local(ActivatorID)_CurrentPointList` = 0;
            }
        }
        code::call_function(p"MoveEndMPlat2");
    }
}

function MoveEndMPlat2{
