process DisplayLvlScore{
    if(s`%var_local(DisplayLvlScoreName)`.equals([0])){
        s`%var_local(DisplayLvlScoreName)` = variable::create_map();
    }
    LvlScoreList = s`%var_local(DisplayLvlScoreName)`.sort_map("ASCENDING", "VALUES");
