            }
        }
        if(g`%var_local(SpiderBodyUUID)_Level`.equals([-35])){
            code::call_function(p"SpiderEnterBar2");
        }
    }
}

event<entity_spawn>{
    if(entity::is_type([item("minecraft:chicken_spawn_egg"), item("minecraft:piglin_spawn_egg"), item("minecraft:zombified_piglin_spawn_egg")])){
        world::cancel_event();
        code::break();
    }
    if(entity::is_type([item("minecraft:strider_spawn_egg")])){
        if(entity::is_riding_entity(["strider"])){
            world::cancel_event();
            code::break();
        }
    }
    code::wait(1);
    if(entity::exists()){
        if(value::name<projectile>.text_starts_with(["turret*"])){
            BulletUUID = value::uuid<default_entity>;
            if(g`%var_local(BulletUUID)_PartType`.equals(["rocket"])){
