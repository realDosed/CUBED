function SpiderLegStraight{
    `%var_local(AnInd)_-1_TipPos` = `%var_local(AnInd)_LegOriginPos`;
    StraightDiretion = variable::get_vector_between_locations(`%var_local(AnInd)_LegOriginPos`, EndPoint);
    SegUpVect = variable::vector_cross_product(vector(0, 1, 0), StraightDiretion);
    SegUpVect = SegUpVect.set_vector_length(1);
    StraightDiretion1 = StraightDiretion.rotate_vector_around_vector(SegUpVect, -90);
    StraightDiretion2 = StraightDiretion.rotate_vector_around_vector(SegUpVect, 90);
    Target = `%var_local(AnInd)_LegOriginPos`;
    repeat::on_range(N, 0, "%math(%var_local(SpiderLocalSegmentCount)-1)"){
        code::call_function(p"WAIT");
        StraightDiretion = StraightDiretion2;
        if(N.equals([0])){
            StraightDiretion = StraightDiretion1;
        }
        code::call_function(p"SpideLegSize");
        `%var_local(AnInd)_%var_local(N)_TipPos` = Target.set_location_direction(StraightDiretion);
        `%var_local(AnInd)_%var_local(N)_TipPos` = `%var_local(AnInd)_%var_local(N)_TipPos`.shift_location_in_direction(LocalLegLengthOverride);
        `%var_local(AnInd)_%var_local(N)_TipPos` = `%var_local(AnInd)_%var_local(N)_TipPos`.face_location(Target);
        Target = `%var_local(AnInd)_%var_local(N)_TipPos`;
    }
}

function SpiderLegFABRIK{
    code::call_function(p"SpiderLegStraight");
    repeat::multi_times(I, 10){
        `%var_local(AnInd)_%math(%var_local(SpiderLocalSegmentCount)-1)_TipPos` = EndPoint;
        repeat::on_range(N, "%math(%var_local(SpiderLocalSegmentCount)-1)", 0, -1){
            code::call_function(p"WAIT");
            code::call_function(p"SpideLegSize");
            `N-1` = variable::subtract([N, 1]);
            `%var_local(AnInd)_%var_local(N)_TipPos` = `%var_local(AnInd)_%var_local(N)_TipPos`.face_location(`%var_local(AnInd)_%var_local(N-1)_TipPos`);
            `%var_local(AnInd)_%var_local(N-1)_TipPos` = `%var_local(AnInd)_%var_local(N)_TipPos`.shift_location_in_direction(LocalLegLengthOverride);
        }
        `%var_local(AnInd)_-1_TipPos` = `%var_local(AnInd)_LegOriginPos`;
        repeat::on_range(N, 0, "%math(%var_local(SpiderLocalSegmentCount)-1)"){
            code::call_function(p"WAIT");
            code::call_function(p"SpideLegSize");
            `N-1` = variable::subtract([N, 1]);
            `%var_local(AnInd)_%var_local(N)_TipPos` = `%var_local(AnInd)_%var_local(N-1)_TipPos`.face_location(`%var_local(AnInd)_%var_local(N)_TipPos`);
            `%var_local(AnInd)_%var_local(N)_TipPos` = `%var_local(AnInd)_%var_local(N)_TipPos`.shift_location_in_direction(LocalLegLengthOverride);
            `%var_local(AnInd)_%var_local(N)_TipPos` = `%var_local(AnInd)_%var_local(N)_TipPos`.face_location(`%var_local(AnInd)_%var_local(N-1)_TipPos`);
        }
        SegDist = variable::locations_distance(`%var_local(AnInd)_%math(%var_local(SpiderLocalSegmentCount)-1)_TipPos`, EndPoint);
        if(SegDist.less(0.01)){
