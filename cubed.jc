    if(BlockType.text_contains(["red_terracotta", "redstone_block"])){
        if(BlockPlaced.equals("0")){
            BlockPlaced = "1";
            RememberPos = FinalBlockPos;
            world::set_block(RememberPos, "minecraft:redstone_block", "TRUE");
